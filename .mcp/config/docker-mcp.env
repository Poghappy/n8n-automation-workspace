# Docker MCP Environment Variables

# MCP 模式设置
MCP_MODE=docker
MCP_LOG_LEVEL=info

# Docker 配置
DOCKER_HOST=unix:///Users/zhiledeng/.docker/run/docker.sock
DOCKER_API_VERSION=1.41
DOCKER_TIMEOUT=30

# 容器配置
DOCKER_NETWORK=bridge
DOCKER_PULL_POLICY=missing
DOCKER_CLEANUP=true

# 安全设置
DOCKER_PRIVILEGED=false
DOCKER_READ_ONLY=false

# 调试设置
DEBUG_MODE=false
TRACE_MODE=false
